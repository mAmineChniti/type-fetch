{
	"name": "@thatguyjamal/type-fetch",
	"version": "0.2.0",
	"description": "A lightweight, zero-dependency HTTP client library for making API requests in JavaScript/TypeScript.",
	"repository": {
		"type": "git",
		"url": "https://github.com/thatguyjamal/type-fetch.git"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": ["dist", "README.md", "LICENSE"],
	"scripts": {
		"build": "tsc -p ./tsconfig.json && tsc-alias -p ./tsconfig.json",
		"ts:init": "tsc --init",
		"format": "biome format --write .",
		"lint": "biome lint --write .",
		"test": "jest"
	},
	"keywords": ["http", "client", "typescript", "library", "request", "fetch"],
	"author": "Jamal Lyons",
	"license": "MIT",
	"sideEffects": false,
	"engines": {
		"node": ">=14.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@jest/types": "^29.6.3",
		"@types/jest": "^29.5.12",
		"@types/node": "22.13.13",
		"jest": "^29.7.0",
		"ts-jest": "29.3.0",
		"ts-node": "^10.9.2",
		"tsc-alias": "^1.8.8",
		"tsconfig-paths-webpack-plugin": "^4.1.0",
		"typescript": "^5.5.4"
	}
}
